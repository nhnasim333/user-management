{
  "version": 2,
  "buildCommand": "npx prisma generate && npm run build",
  "installCommand": "npm install --legacy-peer-deps",
  "framework": null,
  "functions": {
    "api/serverless.ts": {
      "memory": 1024,
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/api/serverless"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}